{
  "manifest_version": 3,
  "name": "Minha Extensão de Nova Guia",
  "version": "1.2",
  "description": "Modifica a página da nova guia.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "background": {
    "service_worker": "script/background.js",
    "type": "module"
  },
  "offline_enabled": false,
  "chrome_url_overrides": {
    "newtab": "pages/index.html"
  },
  "action": {
    "default_popup": "pages/popup.html"
  }
}