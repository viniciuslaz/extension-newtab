{
  "manifest_version": 3,
  "name": "Minha Extensão de Nova Guia",
  "version": "1.0",
  "description": "Modifica a página da nova guia.",
  "permissions": [
    "tabs", "storage"
  ],
  "background": {
    "service_worker": "script/main.js",
    "type": "module"
  },
  "chrome_url_overrides": {
    "newtab": "pages/index.html"
  },
  "action": {
    "default_popup": "pages/popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script/content.js"
      ]
    }
  ]
}